  "name": "blc-mock-store",
  "version": "1.0.0",
  "description": "Ecommerce store",
  "main": "index.js",
  "scripts": {
    "start": "node server --ignore client",
    "develop": "concurrently \"cd server && npm run server\" \"cd client && npm start\"",
